{
   "name": "Page Test",
   "version": "1.0",
   "description": "Build an Extension!",
   "background": {
     "scripts": ["background.js"],
     "persistent": false,
     "run_at": "document_end"
   },
   "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["content.js"]
    }],
   "permissions": ["activeTab", "<all_urls>"],
   "manifest_version": 2
 }
