{
  "name": "Page Test",
  "version": "1.0",
  "description": "Build an Extension!",
  "background": {
    "scripts": ["js/background.js", "js/content.js"],
    "persistent": false,
    "run_at": "document_end"
  },
  "content_scripts": [{
   "matches": ["<all_urls>"],
   "js": ["js/content.js"]
   }],
  "permissions": ["activeTab", "<all_urls>"],
  "manifest_version": 2
}